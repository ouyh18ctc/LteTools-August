@{
    ViewBag.Title = "后台数据管理";
    ViewBag.Message = "——基础信息导入管理、本网页联系人信息";
}
@section featured {
    <hgroup class="title">
        <h1>@ViewBag.Title.</h1>
        <h2>@ViewBag.Message</h2>
    </hgroup>
    <p>
        在这里，我们为数据管理人员提供小规模的数据录入功能（大量的数据录入需要使用应用程序）
    </p>
}
<h3>我们提供以下功能：</h3>
<div class="row">
    <div class="col-md-4">
        <div class="one">
            <h4>行政区域管理</h4>
            <p>
                对全网的地市、区域和镇区信息的增删、查询请点击
                @Html.ActionLink("行政区域管理", "Region", "Region") 
            </p>
        </div>
    </div>
    <div class="col-md-4">
        <div class="two">
            <h4>校园网基本信息导入</h4>
            <p>
                
            </p>
        </div>
    </div>
    <div class="col-md-4">
        <div class="three">
            <h4>日常指标导入</h4>
            <p>
                对全网分片区的2G、3G忙时KPI指标的导入请点击
                @Html.ActionLink("日常忙时指标导入", "Import", "KPI")
            </p>
        </div>
    </div>
    <div class="col-md-4">
        <div class="four">
            <h4>联系人信息</h4>
            <p>
                本网页的系统支持信息请点击
                @Html.ActionLink("技术支持信息", "Contact", "Manage")
            </p>        
        </div>
    </div>
</div>