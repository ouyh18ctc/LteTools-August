@model IList<Lte.Evaluations.Entities.StatValueInterval>

<table summary="User Grid" class="zebra">
    <thead>
        <tr style="font-weight:bold; background-color:#A8C3CB;">
	        <th>区间下限</th>
            <th>区间上限</th>
            <th>区间颜色</th>
        </tr>
    </thead>
    <tbody id="intervals">
    @for (int i = 0; i < Model.Count; i++)
    { 
        <tr>
            <td>@Model[i].IntervalLowLevel</td>
            <td>@Model[i].IntervalUpLevel</td>
            <td style="background-color:#@Model[i].Color.ColorStringForHtml">
            #@Model[i].Color.ColorStringForHtml</td>
        </tr>
    }
    </tbody>
</table>